@model PersonListViewModel
@using SilvagenumWebApp.Pages.App

<h1>@Model.CurrentTree</h1>
@foreach(var person in Model.People)
{
    <component type="typeof(PersonCard)" render-mode="WebAssemblyPrerendered" param-Person=person param-SelectionType="@("none")" param-RelatedId="0"/>
    <script src="_framework/blazor.webassembly.js"></script>
}